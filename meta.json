{
  "prompts": {
    "name": {
      "type": "string",
      "message": "项目名称"
    },
    "appId": {
      "type": "string",
      "message": "appId"
    },
    "description": {
      "type": "string",
      "message": "项目介绍",
      "default": "A BabyTree project"
    },
    "author": {
      "type": "string",
      "message": "作者"
    },
    "lint": {
      "type": "confirm",
      "message": "使用ESLint?",
      "default": false
    },
    "ald": {
      "type": "confirm",
      "message": "使用阿拉丁统计?",
      "default": true
    }
  },
  "filters": {
    ".eslintignore": "lint",
    ".eslintrc.js": "lint",
    "src/utils/ald-stat.js": "ald",
    "src/utils/ald-stat-conf.js": "ald"
  }
}
