{
  "prompts": {
    "name": {
      "type": "string",
      "message": "项目名称"
    },
    "appId": {
      "type": "string",
      "message": "appId"
    },
    "description": {
      "type": "string",
      "message": "项目介绍",
      "default": "A BabyTree project"
    },
    "author": {
      "type": "string",
      "message": "作者"
    },
    "lint": {
      "type": "confirm",
      "message": "使用ESLint?",
      "default": false
    }
  },
  "filters": {
    ".eslintignore": "lint",
    ".eslintrc.js": "lint"
  }
}
